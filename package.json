{
  "name": "pet-care-management-app",
  "version": "1.0.0",
  "description": "A comprehensive pet care management web application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup-admin": "node scripts/createAdmin.js",
    "kill-port": "npx kill-port 3000",
    "test": "npm run test:unit",
    "test:unit": "jest tests/unit/ --verbose",
    "test:unit:models": "jest tests/unit/models/ --verbose",
    "test:unit:services": "jest tests/unit/services/ --verbose",
    "test:integration": "jest tests/integration/ --verbose --testTimeout=30000",
    "test:integration:notifications": "jest tests/integration/api/notifications/ --verbose",
    "test:integration:calendar": "jest tests/integration/api/calendar/ --verbose",
    "test:integration:auth": "jest tests/integration/api/auth/ --verbose",
    "test:e2e:node:auth": "node tests/e2e/nodejs/auth-tests.js",
    "test:e2e:node:pets": "node tests/e2e/nodejs/pet-tests.js",
    "test:python": "python -m pytest tests/python/ -v",
    "test:runners:auth": "node tests/runners/run-auth-tests.js",
    "test:runners:pets": "node tests/runners/run-pet-tests.js",
    "test:runners:notifications": "node tests/runners/notification-test-runner.js",
    "test:legacy:week7": "jest tests/legacy/week7-community-test.js --verbose",
    "test:runners:all": "node tests/runners/simple-master-runner.js",
    "test:week5": "node scripts/test-week5.js",
    "test:week5:services": "node services/test-week5.js",
    "test:watch": "jest tests/unit/ --watch",
    "test:auth": "npm run test:integration:auth && npm run test:e2e:node:auth",
    "test:notifications:model": "jest tests/unit/models/notification-model.test.js --verbose",
    "test:notifications:service": "jest tests/unit/services/notification-service.test.js --verbose",
    "test:notifications:routes": "jest tests/integration/api/notifications/notification-routes.test.js --verbose",
    "test:notifications:integration": "jest tests/integration/api/notifications/notification-integration.test.js --verbose",
    "test:notifications": "npm run test:notifications:model && npm run test:notifications:service && npm run test:notifications:integration",
    "test:tasks:api": "jest tests/integration/api/tasks/task-api.test.js --verbose",
    "test:tasks:integration": "jest tests/integration/api/tasks/task-integration.test.js --verbose",
    "test:calendar:integration": "jest tests/integration/api/calendar/calendar-ui-integration.test.js --verbose",
    "test:e2e:cypress": "cypress run --spec 'tests/e2e/cypress/**/*.cy.js'",
    "test:e2e:cypress:calendar": "cypress run --spec 'tests/e2e/cypress/features/calendar/*.cy.js'",
    "test:e2e:cypress:tasks": "cypress run --spec 'tests/e2e/cypress/features/tasks/*.cy.js'",
    "test:e2e:cypress:notifications": "cypress run --spec 'tests/e2e/cypress/features/notifications/*.cy.js'",
    "test:accessibility": "cypress run --spec 'tests/e2e/cypress/accessibility/*.cy.js'",
    "cypress:open": "cypress open --e2e --spec 'tests/e2e/cypress/**/*.cy.js'"
  },
  "keywords": [
    "pet",
    "care",
    "management",
    "nodejs",
    "mysql"
  ],
  "author": "Ali Abubaker",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "autocannon": "^8.0.0",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.once": "^4.1.1",
    "multer": "^2.0.2",
    "mysql2": "^3.15.0",
    "nodemailer": "^6.10.1",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.1.0",
    "axe-core": "^4.11.0",
    "axios": "^1.13.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "cypress-axe": "^1.7.0",
    "helmet": "^8.1.0",
    "jest": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "method-override": "^3.0.0",
    "nodemon": "^3.0.2",
    "puppeteer": "^21.11.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "start-server-and-test": "^2.1.2",
    "supertest": "^6.3.4",
    "webpack": "^5.102.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
